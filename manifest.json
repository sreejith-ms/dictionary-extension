{
	"manifest_version": 2,
	"version": "2.8",
	"name": "English Dictionary",
	"description": "English Malayalam dictionary",

	"browser_action": {
		"default_icon": "19.png",
		"default_popup": "index.html",
		"default_title": "English Malayalam dictionary"
	},
	"icons": {
		"16": "16.png",
		"128": "128.png"
	},
	"permissions": [
		"http://olam.in/",
		"https://googledictionaryapi.eu-gb.mybluemix.net",
		"storage"
	]
}